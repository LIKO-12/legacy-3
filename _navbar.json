<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    
    
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>_navbar.json</title>

    <link rel="shortcut icon" href="favicon.ico">
    <link rel="stylesheet" href="assets/css/normalize.css">
    <link rel="stylesheet" href="assets/css/main.css">
</head>

<body>
    <nav class="navbar">
        <!-- LIKO-12's Logo -->
        <dev class="navbar-logo">
            <img src="assets/img/logo-icon.svg" class="logo-icon">
            <img src="assets/img/logo-title.svg" class="logo-title">

            <dev class="mobile-spacer"></dev>
            <button class="mobile-toggle"></button>

            <script>
                const navbarElement = document.querySelector(".navbar");
                const toggleButton = document.querySelector(".navbar .mobile-toggle");
                toggleButton.onclick = () => navbarElement.classList.toggle("active");
            </script>
        </dev>

        <!-- Naviagtion links -->

        <!-- Spacer -->
        <dev class="spacer"></dev>

        <!-- Search box -->
        <input type="text" class="search-box" placeholder="Search documentation" autocomplete>
    </nav>
    <div class="main-container">
        <nav class="side-panel">
            Hello side panel!
        </nav>
        <article class="content-container">
            

            <p>[
[&quot;Home&quot;, null],
[&quot;Guide&quot;, null],
[&quot;Reference&quot;, null],
[&quot;Community&quot;, null],
[&quot;Blog&quot;, null]
]</p>

        </article>
    </div>
</body>

</html>